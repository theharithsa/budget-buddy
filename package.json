{
    "name": "finbuddy",
    "private": true,
    "version": "2.5.4",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "kill": "fuser -k 5000/tcp",
        "build": "tsc -b --noCheck && vite build",
        "build:tracked": "node scripts/build-tracker.js",
        "start": "node server.js",
        "serve": "npm run build && npm start",
        "serve:tracked": "npm run build:tracked && npm start",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:unit": "vitest run tests/unit",
        "test:integration": "vitest run tests/integration",
        "test:e2e": "vitest run tests/e2e",
        "test:watch": "vitest watch",
        "test:ci": "vitest run --reporter=verbose --coverage",
        "test:pre-deploy": "npm run lint && npm run test:run && npm run build",
        "test:dynatrace": "node scripts/test-dynatrace.js",
        "lint": "eslint .",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "version:bump": "node scripts/version-bump.js",
        "version:major": "npm run version:bump major",
        "version:minor": "npm run version:bump minor",
        "version:patch": "npm run version:bump patch",
        "changelog": "code CHANGELOG.md",
        "deploy:build": "npm run build",
        "deploy:firebase": "firebase deploy --only hosting",
        "deploy": "npm run deploy:build && npm run deploy:firebase",
        "deploy:preview": "npm run deploy:build && firebase hosting:channel:deploy preview"
    },
